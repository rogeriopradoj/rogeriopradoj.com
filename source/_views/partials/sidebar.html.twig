<a target="_blank" href="{{ site.url }}/">
    <img src="{{ site.url }}/assets/images/rogerio/700x700.jpg" class="thumbnail">
</a>
<ul class="list-unstyled social-list">
    <a target="_blank" href="https://www.linkedin.com/in/rogeriopradoj">
        <li class="social-element" id="linkedin">
            <i class="fa fa-linkedin"></i>
        </li>
    </a>
    <a target="_blank" href="http://www.zend.com/en/yellow-pages/ZEND024088">
        <li class="social-element" id="php">
            <i class="fa fa-code"></i>
        </li>
    </a>
    <a target="_blank" href="https://github.com/rogeriopradoj/">
        <li class="social-element" id="github">
            <i class="fa fa-github-alt"></i>
        </li>
    </a>
    <a target="_blank" href="https://plus.google.com/+RogerioPradoDeJesus?rel=author">
        <li class="social-element" id="google">
            <i class="fa fa-google-plus"></i>
        </li>
    </a>
    <a target="_blank" href="https://twitter.com/rogeriopradoj">
        <li class="social-element" id="twitter">
            <i class="fa fa-twitter"></i>
        </li>
    </a>
    <a href="{{ site.url }}/feed/">
        <li class="social-element" id="rss">
            <i class="fa fa-rss"></i>
        </li>
    </a>
</ul>

<div class="well">
    <h4>
        <p>{{ site.title }}</p>
        {% if site.subtitle %}<small>{{ site.subtitle }}</small>{% endif %}
    </h4>
</div>
<div class="well">
    <h4>Arquivo</h4>
    <ul class="nav">
        <li><a class="btn btn-block btn-xs btn-info" href="{{ site.url }}/categories/">Categorias</a></li>
        <li><ul class="list-unstyled">
            <li class="categories-list">
                {% for category,posts in data.posts_categories %}
                    <a class="label label-info" href="{{ site.url }}/category/{{ category | url_encode(true) }}/">{{ category }}</a>
                {% endfor %}
            </li>
            <li class="clearfix"></li>
        </ul></li>
        <li>&nbsp;</li>
        <li><a  class="btn btn-block btn-xs btn-primary" href="{{ site.url }}/tags/">Tags</a></li>
        <li><ul class="list-unstyled">
            <li class="tags-list">
                {% for tag,posts in data.posts_tags %}
                    <a class="label label-primary" href="{{ site.url }}/tag/{{ tag | url_encode(true) }}/">{{ tag | replace('-', ' ') }}</a>
                {% endfor %}
            </li>
            <li class="clearfix"></li>
        </ul></li>
    </ul>
</div>