{
    "name": "rogeriopradoj/rogeriopradoj.com",
    "require": {
        "mavimo/sculpin-redirect-bundle": "@dev",
        "sharkpp/sculpin-calendarian-bundle": "dev-master",
        "sculpin/sculpin": "~2.1@dev"
    },
    "config": {
        "component-dir": "source/assets/components"
    },
    "autoload": {
        "psr-0": {
            "AndrewShell": "app/src",
            "Acelaya": "app/src"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "serve": "sculpin generate --watch --server"
    }
}
